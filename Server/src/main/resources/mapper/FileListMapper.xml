<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.weblab.server.mapper.FileListMapper">

    <select id="getFileIds" resultType="java.lang.Long">
        select
            file_list.file_id
        from file_list
        where file_list.file_role = ${fileRole} and file_list.node_id = ${nodeId}
    </select>
</mapper>